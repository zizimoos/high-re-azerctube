mixin mVideo(data)
    a(href=`/video/${data.id}`).video-mixin
        div.video-mixin__thumb(style=`background-image: url(${data.thumbUrl});background-size:cover;background-position:center;`)
        div.video-mixin__data
            span.video-mixin__title=data.title
            div.video-mixin__meta
                span #{data.owner.name} • 
                span #{data.meta.views} 회
            